  "version": "0.2.0",
  "configurations": [
    {
      "name": "svarog sync files",
      "type": "debugpy",
      "request": "launch",
      "module": "svarog.cli",
      "args": [
        "sync",
        "files",
        ".scratchpad/a.yaml",
        ".scratchpad/b.yml",
        "--section",
        "yaml:*->yaml:my_section",
        "--dry-run",
        "--diff"
      ]
    }
  ]
}
